# This file is a template for .env file
# Copy this file to .env and change the values

# Web server configuration
HOST="localhost"
PORT="3030"

# Public URL to proxy ts files from
PUBLIC_URL="https://m3u8.eltik.net"

# IPv6 rotation configuration
USE_IPV6_ROTATION=true
IPV6_PREFIX=2001:db8:1234        # Your IPv6 prefix (usually provided by ISP)
IPV6_SUBNET=1000                 # Subnet within your prefix
IPV6_INTERFACE=eth0               # Your network interface (eth0, en0, etc.)
IPV6_POOL_SIZE=20                # Number of IPv6 addresses to maintain
IPV6_POOL_INTERVAL=5000          # How often to refresh IPs (milliseconds)
IPV6_BATCH_SIZE=5                # How many IPs to add/remove at once
IPV6_DEBUG=true                  # Enable debug logging